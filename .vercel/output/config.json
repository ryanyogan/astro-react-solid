{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "about.md"
		},
		{
			"src": "^\\/shop\\/?$",
			"dest": "shop.astro"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "404.astro"
		}
	]
}